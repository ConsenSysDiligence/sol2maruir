{
    "name": "@consensys/sol2maruir",
    "version": "0.0.1",
    "description": "Library (and command-line tool) for transpiling Solidity to MaruIR",
    "keywords": [],
    "files": [
        "dist"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "sol2maruir": "dist/bin/sol2maruir.js"
    },
    "scripts": {
        "clean": "rm -rf dist/",
        "transpile": "tsc",
        "build": "npm run clean && npm run transpile && chmod a+x dist/bin/sol2maruir.js",
        "lint": "eslint src/ test/ --ext=ts",
        "lint:fix": "eslint src/ test/ --ext=ts --fix",
        "test": "nyc mocha",
        "prepare": "npm run build"
    },
    "dependencies": {
        "fs-extra": "^11.2.0",
        "logplease": "1.2.15",
        "minimist": "^1.2.8",
        "semver": "^7.6.0",
        "maru-ir2": "git+ssh://git@github.com:Consensys/maru-ir2.git",
        "solc-typed-ast": "^18.1.2",
        "web3-utils": "^4.2.0"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "@types/minimist": "^1.2.5",
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.11.19",
        "@types/semver": "^7.5.7",
        "@typescript-eslint/eslint-plugin": "^7.0.2",
        "@typescript-eslint/parser": "^7.0.2",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "expect": "^29.7.0",
        "mocha": "^10.3.0",
        "nyc": "^15.1.0",
        "prettier": "^3.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
